<?xml version="1.0"?><modification>	<id><![CDATA[MoiP Checkout Transparente]]></id>	<version><![CDATA[1.0]]></version>	<vqmver><![CDATA[1.1.6]]></vqmver>	<author><![CDATA[valdeir <valdeirpsr@hotmail.com>]]></author>	<file name="admin/controller/common/header.php">		<operation>			<search position="after"><![CDATA[$this->data['text_zone'] = $this->language->get('text_zone');]]></search>			<add><![CDATA[								$this->data['moip_notify'] = $this->config->get('moip_notify');								if ($this->data['moip_notify'] == 1):					if(isset($this->session->data['token'])):						$token = $this->session->data['token'];					else:						$token = '';					endif;										$this->load->language('moip/header');					$this->data['text_moip'] = $this->language->get('header_text_moip');					$this->data['text_pedidos'] = $this->language->get('header_text_pedidos');					$this->data['text_documentacao'] = $this->language->get('header_text_documentacao');					$this->data['text_faq'] = $this->language->get('header_text_faq');					$this->data['text_desinstalar'] = $this->language->get('header_text_desinstalar');					$this->data['link_pedidos'] = $this->url->link('moip/moip', 'token=' . $token);					$this->data['link_faq'] = HTTPS_CATALOG.'valdeir/moip/documentacao/';					$this->data['link_desinstalar'] = HTTPS_CATALOG.'valdeir/moip/desinstalar/';				endif;			]]></add>		</operation>	</file><file name="admin/view/template/common/header.tpl">		<operation>			<search position="before"><![CDATA[<li id="help"><a class="top"><?php echo $text_help; ?></a>]]></search>			<add><![CDATA[				<?php if ($moip_notify){ ?>				<li id="moip"><a class="top"><?php echo $text_moip; ?></a>					<ul>					  <li><a href="<?php echo $link_pedidos; ?>"><?php echo $text_pedidos; ?></a></li>					  <li><a href="<?php echo $link_faq; ?>"><?php echo $text_faq; ?></a></li>					  <li><a href="<?php echo $link_desinstalar; ?>"><?php echo $text_desinstalar; ?></a></li>					</ul>				  </li>				  <?php } ?>			]]></add>		</operation>	</file>	<file name="catalog/view/theme/*/template/common/header.tpl">		<operation>			<search position="replace"><![CDATA[ <?php echo $google_analytics; ?> ]]></search>			<add><![CDATA[<?php echo $google_analytics; ?><script type="text/javascript" src="<?php echo $MoiPJS; ?>"></script>]]></add>		</operation>	</file>	<file name="catalog/controller/common/header.php">		<operation>			<search position="after"><![CDATA[$this->data['title'] = $this->document->getTitle();]]></search>			<add><![CDATA[//Verifica se estÃ¡ em modo de teste		if (!$this->config->get('moip_test')) {    		$this->data['MoiPJS'] = 'https://www.moip.com.br/transparente/MoipWidget-v2.js';  		} else {			$this->data['MoiPJS'] = 'https://desenvolvedor.moip.com.br/sandbox/transparente/MoipWidget-v2.js';		}]]></add>		</operation>	</file></modification>